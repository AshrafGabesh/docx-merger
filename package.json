{
  "name": "docx-merger",
  "version": "0.0.0-development",
  "license": "MIT",
  "description": "Javascript Library for Merging Docx file in NodeJS and Browser Environment.",
  "main": "dist/docx-merger.js",
  "types": "dist/index.d.ts",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "cache": false,
    "lines": 80,
    "statements": 80,
    "functions": 90,
    "branches": 90,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage/**",
      "packages/*/test/**",
      "test/**",
      "test{,-*}.js",
      "**/*{.,-}test.js",
      "**/__tests__/**",
      "**/node_modules/**",
      "**/webpack.config.js"
    ],
    "reporter": [
      "text-lcov"
    ],
    "all": true
  },
  "scripts": {
    "commit": "git-cz",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coverage": "nyc report --reporter=text-lcov | npx codecov --pipe",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "postbuild": "npm run build:declarations",
    "build:declarations": "tsc --project ./tsconfig.build.json",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apurvaojas/docx-merger.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "docx",
    "composition",
    "document",
    "office",
    "docx merge",
    "microsoft word"
  ],
  "author": "Apurv Ojas <apurvaojas10@gmail.com>",
  "dependencies": {
    "@types/jsdom": "^12.2.4",
    "jsdom": "^15.1.1",
    "jszip": "^3.2.1",
    "linq": "^3.2.0",
    "ltxml": "github:jthuraisamy/ltxml",
    "xmldom": "^0.1.27"
  },
  "bugs": {
    "url": "https://github.com/apurvaojas/docx-merger/issues"
  },
  "homepage": "https://github.com/apurvaojas/docx-merger#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "[[ -n $HUSKY_BYPASS ]] || commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/commit-analyzer": "^6.1.0",
    "@semantic-release/git": "^7.0.8",
    "@semantic-release/github": "^5.2.10",
    "@semantic-release/npm": "^5.1.4",
    "@semantic-release/release-notes-generator": "^7.1.4",
    "@types/chai": "^4.1.7",
    "@types/jszip": "^3.1.5",
    "@types/mocha": "^5.2.6",
    "@types/node": "^10.12.18",
    "@types/sinon": "^7.0.13",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "codecov": "^3.3.0",
    "commitizen": "^3.0.5",
    "conventional-changelog-angular": "^5.0.3",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "husky": "^1.3.1",
    "mocha": "^6.1.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.3.0",
    "prettier": "^1.16.0",
    "puppeteer": "^1.14.0",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "semantic-release-cli": "^4.1.2",
    "sinon": "^7.3.1",
    "ts-node": "^8.0.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.4",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1"
  }
}
